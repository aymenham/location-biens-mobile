<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.ekrili">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.MainActivity" />
        <activity
            android:name="com.HommeScreen"
            android:screenOrientation="portrait"
            android:theme="@style/SimpleThem">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.LoginActivity"
            android:theme="@style/SimpleThem" />
        <activity
            android:name="com.RegisterActivity"
            android:theme="@style/SimpleThem" />
        <activity android:name="com.ResultSearchActivity" />
        <activity
            android:name="com.BienDetailleActivity"
            android:theme="@style/SimpleThem" />
        <activity
            android:name=".ClientSpaceActivity"
            android:label="@string/title_activity_client_space"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ValiderBienActivity"></activity>
    </application>

</manifest>